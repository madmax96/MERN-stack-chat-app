{
    "extends": "airbnb",
    "env":{
        "es6":true,
        "browser":true,
        "node":true,
        "mocha":true,
        "mongo":true
    },
    "rules":{
        "linebreak-style": 1,
        "no-underscore-dangle": 1,
        "no-param-reassign": 1,
        "consistent-return": 1,
        "no-unused-expressions":1,
        "no-debugger":0,
        "no-shadow": 0,
        "eqeqeq":1
    }
}